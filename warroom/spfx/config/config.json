{
  "$schema": "https://dev.office.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "army-overview-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/armyOverview/ArmyOverviewWebPart.js",
          "manifest": "./src/webparts/armyOverview/ArmyOverviewWebPart.manifest.json"
        }
      ]
    },
    "war-room-control-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/warRoomControl/WarRoomControlWebPart.js",
          "manifest": "./src/webparts/warRoomControl/WarRoomControlWebPart.manifest.json"
        }
      ]
    },
    "level-up-units-command-set": {
      "components": [
        {
          "entrypoint": "./lib/extensions/levelUpUnits/LevelUpUnitsCommandSet.js",
          "manifest": "./src/extensions/levelUpUnits/LevelUpUnitsCommandSet.manifest.json"
        }
      ]
    },
    "squire-chat-control-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/squireChatControl/SquireChatControlWebPart.js",
          "manifest": "./src/webparts/squireChatControl/SquireChatControlWebPart.manifest.json"
        }
      ]
    },
    "battle-commands-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/battleCommands/BattleCommandsWebPart.js",
          "manifest": "./src/webparts/battleCommands/BattleCommandsWebPart.manifest.json"
        }
      ]
    },
    "battle-map-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/battleMap/BattleMapWebPart.js",
          "manifest": "./src/webparts/battleMap/BattleMapWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "ControlStrings": "./node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "ArmyOverviewWebPartStrings": "lib/webparts/armyOverview/loc/{locale}.js",
    "WarRoomControlWebPartStrings": "lib/webparts/warRoomControl/loc/{locale}.js",
    "LevelUpUnitsCommandSetStrings": "lib/extensions/levelUpUnits/loc/{locale}.js",
    "SquireChatControlWebPartStrings": "lib/webparts/squireChatControl/loc/{locale}.js",
    "BattleCommandsWebPartStrings": "lib/webparts/battleCommands/loc/{locale}.js",
    "BattleMapWebPartStrings": "lib/webparts/battleMap/loc/{locale}.js"
  }
}
